<h3>Index</h3>

<p>{{link-to 'New person' 'people.new'}}</p>

<table>
  <tr>
    <th>id</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>City</th>
    <th>State</th>
    <th>Email</th>
  </tr>
  {{#each}}
    <tr>
      <td>{{link-to id 'people.show' this}}</td>
      <td>{{firstname}}</td>
      <td>{{lastname}}</td>
      <td>{{city}}</td>
      <td>{{state}}</td>
      <td>{{email}}</td>
    </tr>
  {{/each}}
</table>
